<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="MainWindow" parent="GtkApplicationWindow">
    <property name="title">Client</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">horizontal</property>
        <child>
          <object class="GtkStackSidebar">
            <property name="stack">chats_stack</property>
          </object>
        </child>
        <child>
          <object class="GtkStack" id="chats_stack">
            <property name="width-request">300</property>
            <property name="height-request">500</property>
            <child>
              <object class="GtkStackPage">
                <property name="name">chat1</property>
                <property name="title">Chat1</property>
                <property name="child">
                  <object class="Chat">
                  </object>
                </property>
              </object>
            </child>
            <child>
              <object class="GtkStackPage">
                <property name="name">chat2</property>
                <property name="title">Chat2</property>
                <property name="child">
                  <object class="Chat">
                  </object>
                </property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
